package week5;

public class FUNNEL {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for (int i=5; i>=1; i--){
			for(int j=1; j<=i;j++){
				if(i==5){
				System.out.print("*");
				System.out.print(" ");}
				else
				{System.out.print(" ");
	             System.out.print("*");
	    }
		}
					}
	}
}
			
		

	



package programming2Week2;

public class CreateAlarm {

	public static void main (String arg[]) {
	     Alarm A1 = new Alarm();
	     Alarm A2 = new Alarm(true, "5:00","Hey");
	     Alarm A3 = new Alarm(false, "7:00","off");
	     
	     System.out.println(A2.toString());
        
	     
	}
	
	
	
}

